{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/acceptance-rules/(.*)",
      "dest": "/api/acceptance-rules.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
