{
  "version": 2,
  "functions": {
    "api/explain_rule": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/**/*.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
